{
  "name": "mobile-wedding-invite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && node scripts/postexport.mjs",
    "deploy:gh": "npm run export && npx gh-pages -d out -b gh-pages",
    "deploy:s3:dry": "aws s3 sync out/ s3://$S3_BUCKET --delete --dryrun",
    "deploy:s3": "npm run export && aws s3 sync out/ s3://$S3_BUCKET --delete"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  }
}
